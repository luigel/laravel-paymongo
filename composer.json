{
    "name": "luigel/laravel-paymongo",
    "description": "A laravel wrapper for Paymongo API",
    "keywords": [
        "luigel",
        "laravel-paymongo",
        "laravel",
        "paymongo",
        "gcash",
        "credit-cards",
        "debit-cards",
        "grabcar",
        "payment-gateway"
    ],
    "homepage": "https://github.com/luigel/laravel-paymongo",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rigel Kent Carbonel",
            "email": "rigel20.kent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-parallel": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "Luigel\\Paymongo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luigel\\Paymongo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luigel\\Paymongo\\PaymongoServiceProvider"
            ],
            "aliases": {
                "Paymongo": "Luigel\\Paymongo\\Facades\\Paymongo"
            }
        }
    }
}
